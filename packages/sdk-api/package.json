    "dist",
    "out"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "test:ci": "echo \"Error: no test specified\" && exit 0",
    "build": "rm -rf dist/ && cross-env NODE_ENV=production rollup --config config/rollup.config.js",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings=0",
    "gen:openapi": "rm -rf ./src/api/models/* && npx openapi-typescript-codegen -i https://api.dev.monite.com/openapi.json?version=2022-11-16 --exportCore false --exportServices false -o ./src/api",
    "gen:onboarding": "rm -rf ./src/onboarding/models/* && npx openapi-typescript-codegen -i openapi.json --exportCore false --exportServices false -o ./src/onboarding"
