{
  "name": "@team-monite/gitlab-sync",
  "private": true,
  "packageManager": "yarn@3.5.0",
  "bin": "bin.mjs",
  "scripts": {
    "dev": "tsc --watch --project tsconfig.build.json --noEmitOnError false",
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf build/",
    "lint": "eslint src --ext ts,tsx,js,jsx,cjs,mjs --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "cli": "node bin.mjs"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./build/index.js",
    "./package.json": "./package.json",
    "./*": "./build/*.js"
  },
  "dependencies": {
    "@gitbeaker/rest": "~40.0.2",
    "chalk": "~5.3.0",
    "commander": "~12.0.0",
    "dotenv": "~16.4.1"
  },
  "devDependencies": {
    "@team-monite/eslint-plugin": "workspace:~",
    "eslint": "~8.38.0",
    "typescript": "~5.0.4"
  },
  "peerDependencies": {
    "@gitbeaker/requester-utils": "~40.0.2"
  }
}
