{
  "name": "@monite/js-sdk-root",
  "version": "0.0.1",
  "description": "monite js sdk",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "app:start": "yarn workspace @monite/sdk-app start",
    "app:serve": "yarn workspace @monite/sdk-app serve",
    "app:build": "yarn workspace @monite/sdk-app build",
    "app:analyze": "yarn workspace @monite/sdk-app analyze",
    "sdk:build": "yarn workspace @monite/js-sdk build",
    "react:build": "yarn workspace @monite/react-kit build",
    "react:story": "yarn workspace @monite/react-kit storybook",
    "react:build-storybook": "yarn workspace @monite/react-kit build-storybook",
    "ui:story": "yarn workspace @monite/ui storybook",
    "ui:build": "yarn workspace @monite/ui build",
    "ui:build-storybook": "yarn workspace @monite/ui build-storybook",
    "web:build": "yarn workspace @monite/web-kit build",
    "payment:start": "yarn workspace @monite/payment start",
    "payment:build": "yarn workspace @monite/payment build",
    "full:build": "yarn ui:build && yarn sdk:build && yarn react:build",
    "lint": "yarn workspaces run lint",
    "lint-staged": "yarn workspaces run lint-staged",
    "prepare": "husky install"
  },
  "author": "monite.com",
  "license": "ISC",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lerna": "^4.0.0",
    "lint-staged": "~13.0.2",
    "prettier": "^2.6.2"
  }
}
